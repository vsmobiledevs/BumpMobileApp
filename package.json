{
  "name": "BumpMobileApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "setup": "yarn && yarn postinstall",
    "test": "jest",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "openXcode": "cd ios; open ReactNativeCRUD.xcworkspace",
    "pod_installer": "npx pod-install ios",
    "iPhone6s": "react-native run-ios --simulator \"iPhone 6s\"",
    "ios_staging_iphone_6s": "yarn iPhone6s --scheme StagingBookKaru --configuration StagingDebug",
    "iPhone11ProMax": "npx react-native run-ios --simulator \"iPhone 11 Pro Max\"",
    "podCleaner": "cd ios; pod deintegrate; rm -rf Podfile.lock; cd .. ; yarn pod_installer",
    "android_run_staging": "yarn addFlipperReduxIn; npx react-native run-android --variant=stagingDebug",
    "android_run_production": "npx react-native run-android --variant=productionDebug",
    "android_bundle_production": "cd android; ./gradlew bundleProductionRelease && cd app/build/outputs/bundle/productionRelease && cp -R ./ /Desktop && cd /Desktop && open .;",
    "android_bundle_staging": "yarn shotFlipperAway; cd android; ./gradlew bundleStagingRelease && cd app/build/outputs/bundle/stagingRelease && cp -R ./ /Desktop && cd /Desktop && open .;",
    "release_build_android_staging": "yarn shotFlipperAway; cd android && ./gradlew assembleStagingRelease && cd app/build/outputs/apk/staging/release && cp -R ./ /Desktop && cd /Desktop && open .;",
    "release_build_android_production": "cd android && ./gradlew assembleProductionRelease && cd app/build/outputs/apk/production/release && cp -R ./ /Desktop && cd /Desktop && open .;",
    "clean_android_cache": "cd android; ./gradlew clean;",
    "shotFlipperAway": "yarn remove redux-flipper react-native-flipper",
    "addFlipperReduxIn": "yarn add redux-flipper react-native-flipper",
    "rnuc_staging": "$(npm bin)/rnuc .env.staging.yaml",
    "rnuc_production": "$(npm bin)/rnuc .env.yaml",
    "gradlewClean": "cd android && ./gradlew clean && cd ..",
    "assembleRelease": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release && cd ..",
    "bundleRelease": "cd android && ./gradlew bundleRelease && open app/build/",
    "installLibs": "yarn add @react-native-async-storage/async-storage @react-native-community/masked-view @react-navigation/bottom-tabs @react-navigation/native @react-navigation/stack react-native-gesture-handler react-native-reanimated react-native-safe-area-context react-native-screens react-native-elements react-native-vector-icons react-redux redux redux-devtools-extension redux-logger redux-persist redux-saga axios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-seoul/masonry-list": "^1.4.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "d3-shape": "^3.2.0",
    "formik": "^2.2.9",
    "jetify": "^1.0.2",
    "metro-config": "^0.76.5",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-animatable": "^1.3.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^4.7.1",
    "react-native-confirmation-code-field": "^7.3.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-image-picker": "^5.4.0",
    "react-native-indicators": "^0.17.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "^6.2.0",
    "react-native-paper": "^5.8.0",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "^3.2.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-simple-toast": "^1.1.4",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^5.59.9",
    "assert": "^2.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "os-browserify": "^0.3.0",
    "prettier": "2.8.8",
    "react-app-rewired": "^2.2.1",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
